@model PAIProjektAgain.Models.SearchByIdViewModel
@if (User.Identity.IsAuthenticated)
{
    @Html.ActionLink("Podgląd moich zamówień", "MyOrders")
}
<form asp-action="SearchById" method="get">
    <div class="form-group">
        <label class="control-label">Wprowadź numer zamówienia</label>
        <input asp-for="Id" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Szukaj" class="btn btn-primary" />
    </div>
</form>